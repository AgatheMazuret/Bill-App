{
  "scripts": {
    "test": "jest --coverage"
  },
  "jest": {
    "verbose": false,
    "setupFiles": [
      "./setup-jest.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/app/**",
      "!**/assets/**",
      "!**/external/**",
      "!**/fixtures/**",
      "!**/lcov-report/**"
    ],
    "testEnvironment": "jsdom"
  },
  "type": "module",
  "dependencies": {
    "express": "^4.17.1",
    "global": "^4.4.0",
    "jquery": "^3.5.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^12.0.11",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporter": "^3.1.3"
  }
}
